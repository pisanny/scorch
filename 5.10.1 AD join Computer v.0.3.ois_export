    $Zone = '\`d.T.~Vb/{B7776C48-6713-4F95-B723-A5980F4FE147}\`d.T.~Vb/'

    [System.Net.IPAddress]$NewIP = [System.Net.IPAddress]($hostIP)
    $ResourceName = $ResourceName.split(".")[0]

    $ExistingRecord = $(Try {Get-DnsServerResourceRecord -Name $ResourceName -ZoneName $Zone -RRType "A" -ComputerName $dnsserver -ErrorAction Stop} Catch {$null})

    #region New A-Record
    If ($ExistingRecord -eq $null)
    {
        try
        {
            Add-DnsServerResourceRecordA -CreatePtr -Name $ResourceName -ZoneName $Zone -ComputerName $dnsserver -ErrorAction Stop -IPv4Address $NewIP -Confirm:$false # -verbose
        }
        catch
        {
            return $_
        }
    }
    #endregion New A-Record
    #region Change existing A-Record
    Else
    {
        $NewRecord = $ExistingRecord.Clone()
        $NewRecord.RecordData.Ipv4Address = $NewIP
        try
        {
            Set-DnsServerResourceRecord -NewInputObject $NewRecord -OldInputObject $ExistingRecord -ZoneName $Zone -ComputerName $dnsserver
        }
        catch
        {
            return $_
        }
    }
    #endregion Change existing A-Record
        
    $NewRecord = $null
    $ExistingRecord = $null
}</ScriptBody><Dependencies datatype="null"></Dependencies><Namespaces datatype="null"></Namespaces><ExecutionData datatype="string">&lt;ExecutionData&gt;&lt;Item&gt;&lt;Description&gt;&lt;/Description&gt;&lt;Hint&gt;&lt;/Hint&gt;&lt;ID&gt;results&lt;/ID&gt;&lt;Text&gt;results&lt;/Text&gt;&lt;Type&gt;String&lt;/Type&gt;&lt;/Item&gt;&lt;Item&gt;&lt;Description&gt;C#&lt;/Description&gt;&lt;Hint&gt;&lt;/Hint&gt;&lt;ID&gt;ScriptType&lt;/ID&gt;&lt;Text&gt;Script Language&lt;/Text&gt;&lt;Type&gt;String&lt;/Type&gt;&lt;/Item&gt;&lt;/ExecutionData&gt;</ExecutionData><PublishedData datatype="string">&lt;ItemRoot&gt;&lt;Entry&gt;&lt;IsCollection&gt;false&lt;/IsCollection&gt;&lt;Name&gt;results&lt;/Name&gt;&lt;Type&gt;String&lt;/Type&gt;&lt;Variable&gt;results&lt;/Variable&gt;&lt;/Entry&gt;&lt;/ItemRoot&gt;</PublishedData></Object><Object><UniqueID datatype="string">{06699281-348A-49CD-B69C-F395031BBDDB}</UniqueID><ParentID datatype="string">{C24809F4-1E1B-43F2-B1FB-BD5F4D67264A}</ParentID><Name datatype="string">Add service group</Name><Description datatype="null"></Description><PositionX datatype="int">546</PositionX><PositionY datatype="int">77</PositionY><ObjectType datatype="string">{ED7F2A41-107A-4B74-BAFE-ADAE63632B79}</ObjectType><SubType datatype="null"></SubType><Enabled datatype="bool">TRUE</Enabled><Flags datatype="null"></Flags><ASC_UseServiceSecurity datatype="null"></ASC_UseServiceSecurity><ASC_ThisAccount datatype="null"></ASC_ThisAccount><ASC_Username datatype="null"></ASC_Username><ASC_Password datatype="null"></ASC_Password><HasExtenders datatype="null"></HasExtenders><CreationTime datatype="date">131744020590000000</CreationTime><CreatedBy datatype="string">S-1-5-500</CreatedBy><LastModified datatype="date">131824276490000000</LastModified><LastModifiedBy datatype="string">S-1-5-21-1549627856-163330409-526660263-86796</LastModifiedBy><Deleted datatype="bool">FALSE</Deleted><Cost datatype="null"></Cost><Savings datatype="null"></Savings><Number datatype="int