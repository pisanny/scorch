t;&lt;/ExecutionData&gt;</ExecutionData></Object><Object><UniqueID datatype="string">{7FC3BC2F-0220-4AF2-BD0C-7946682A1F75}</UniqueID><ParentID datatype="string">{C24809F4-1E1B-43F2-B1FB-BD5F4D67264A}</ParentID><Name datatype="string">Add DNS PTR-Record</Name><Description datatype="null"></Description><PositionX datatype="int">321</PositionX><PositionY datatype="int">227</PositionY><ObjectType datatype="string">{ED7F2A41-107A-4B74-BAFE-ADAE63632B79}</ObjectType><SubType datatype="null"></SubType><Enabled datatype="bool">TRUE</Enabled><Flags datatype="null"></Flags><ASC_UseServiceSecurity datatype="null"></ASC_UseServiceSecurity><ASC_ThisAccount datatype="null"></ASC_ThisAccount><ASC_Username datatype="null"></ASC_Username><ASC_Password datatype="null"></ASC_Password><HasExtenders datatype="null"></HasExtenders><CreationTime datatype="date">131823564850000000</CreationTime><CreatedBy datatype="string">S-1-5-500</CreatedBy><LastModified datatype="date">131824276490000000</LastModified><LastModifiedBy datatype="string">S-1-5-21-1549627856-163330409-526660263-86796</LastModifiedBy><Deleted datatype="bool">FALSE</Deleted><Cost datatype="null"></Cost><Savings datatype="null"></Savings><Number datatype="int">0</Number><AlternateDisplayData datatype="null"></AlternateDisplayData><ASW_ObjectTimeout datatype="null"></ASW_ObjectTimeout><ASW_NotifyOnFail datatype="null"></ASW_NotifyOnFail><Flatten datatype="bool">FALSE</Flatten><FlatUseLineBreak datatype="bool">FALSE</FlatUseLineBreak><FlatUseCSV datatype="bool">FALSE</FlatUseCSV><FlatUseCustomSep datatype="bool">TRUE</FlatUseCustomSep><FlatCustomSep datatype="string">,</FlatCustomSep><ObjectTypeName datatype="string">Run .Net Script</ObjectTypeName><ScriptType datatype="string">PowerShell</ScriptType><ScriptBody datatype="string">[string]$dnsserver = '\`d.T.~Ed/{0ECBBB1A-B96E-476C-910B-BE3630EA84E3}.activeDC\`d.T.~Ed/'
$servicepassword = ConvertTo-SecureString -String '\`d.T.~Vb/{3DC56297-1FE7-4E3C-B424-0332F01D824E}\`d.T.~Vb/' -AsPlainText -Force
$servicecredential = New-Object system.Management.Automation.PSCredential('\`d.T.~Vb/{B5DDA800-1073-48B6-B940-A633F4ECE9E3}\`d.T.~Vb/', $servicepassword)

$result = Invoke-Command -ComputerName $dnsserver -Credential $servicecredential {
    Import-Module DnsServer

    $ResourceName = '\`d.T.~Ed/{3835F517-CAB4-41DD-A435-3EDF42C71BE0}.{07714F2E-FF00-4C5C-8084-2AA3BF58AC04}\`d.T.~Ed/'
    $hostIP = '\`d.T.~Ed/{3835F517-CAB4-41DD-A435-3EDF42C71BE0}.{F8BBB1F9-B1E6-4ACF-B31E-A0592DD8BB83}\`d.T.~Ed/'
    $dnsserver = '\`d.T.~Ed/{0ECBBB1A-B96E-476C-910B-BE3630EA84E3}.activeDC\`d.T.~Ed/'
    $Zone = '\`d.T.~Vb/{B7776C48-6713-4F95-B723-A5980F4FE147}\`d.T.~Vb/'
    $result = 'Reverse lookup zone not found!'
        
    #region PTR
    $reverseZones = (Get-DnsServerZone -ComputerName $dnsserver | Where {$_.IsReverseLookupZone -match "True" -and $_.IsDsIntegrated -match "True"} | Select ZoneName).ZoneName # Get Reverse Zones, except "fakes" (0.in-addr.arpa, 127.in-addr.arpa, 255.in-addr.arpa)
    $hostName =  $ResourceName.split(".")[0]
    $FQDN = $hostName + '.' + $Zone

    $ExistingPtr = $reverseZones | %{Get-DnsServerResourceRecord -ZoneName $_ -ComputerName $dnsserver -RRType PTR | ?{$_.RecordData.PtrDomainName -match $hostName}}
        
        #region reverse IP Address
        $IPAddressAsArray = $hostIP.Split('.') # Convert IP address to array using "." as delimiter
        [array]::Reverse($IPAddressAsArray)
        $reversedIP = $IPAddressAsArray -join '.' # The name of the PTR record is made of reversed IP Address, so the variable is used to construst the PTR resource record Name
        #endregion reverse IP Address

        #[string]$theReverseZone = $reverseZones -match "^($octet3)?\.?($octet2)?\.?($octet1)\.in-addr\.arpa" | sort Length -Descending | select -First 1
        [string]$theReverseZone = $reverseZones -match "^($($hostAddr.Split('.')[2]))?\.?($($hostAddr.Split('.')[1]))?\.?($($hostAddr.Split('.')[0]))\.in-addr\.arpa" | sort Length -Descending | select -First 1

        #region Create / Change PTR-record
        If( !([string]::IsNullOrWhiteSpace($theReverseZone)) )#if the zone exists
        {        
            $PtrDomainName = $FQDN + '.' # for PTR records "." is added to the domain name

            $ptrRRName = $reversedIP -replace "(.*)$('\.' + $($theReverseZone -replace '\.in-addr\.arpa'))(.*)", '$1$2' # Resource record name. Zone Name subtracted out of reversed IP (the last occurrence of Zone Name). ".in-addr.arpa" subtracted out of zone name.

            $OldObj = $(Try {Get-DnsServerResourceRecord -Node $($ExistingPtr | Select -ExpandProperty HostName | ?{$reversedIP -match $_})  -ZoneName $theReverseZone -RRType "PTR" -ComputerName $dnsserver -ErrorAction SilentlyContinue} Catch {$null})
            If ($OldObj -eq $null)
            {
                #Write-Verbose "Object does not exist in DNS zone $Zone, creating entry now"
                Add-DnsServerResourceRecordPtr -Name $ptrRRName -PtrDomainName $PtrDomainName -ZoneName $theReverseZone -ComputerName $dnsserver # -Verbose
            }
            Else
            {
                $NewObj = Get-DnsServerResourceRecord -Node $($ExistingPtr | Select -ExpandProperty HostName | ?{$reversedIP -match $_})  -ZoneName $theReverseZone -RRType "PTR" -ComputerName $dnsserver
                $NewObj.RecordData.PtrDomainName = $FQDN + '.'
                If (($NewObj.RecordData.PtrDomainName -ine $OldObj.RecordData.PtrDomainName))
                {
                    #Write-Verbose "Objects are different:  old - $OldObj, new - $NewObj. Performing change in DNS"
                    Set-DnsServerResourceRecord -NewInputObject $NewObj -OldInputObject $OldObj -ZoneName $theReverseZone -ComputerName $dnsserver
                }
            }
            $OldObj = $null
            $NewObj = $null

            $result = 'PTR record added'
        }
        #endregion Create / Change PTR-record
            
    #endregion PTR
    return $result
}</ScriptBody><Dependencies datatype="null"></Dependencies><Namespaces datatype="null"></Namespaces><ExecutionData datatype="string">&lt;ExecutionData&gt;&lt;Item&gt;&lt;Description&gt;&lt;/Description&gt;&lt;Hint&gt;&lt;/Hint&gt;&lt;ID&gt;results&lt;/ID&gt;&lt;Text&gt;results&lt;/Text&gt;&lt;Type&gt;String&lt;/Type&gt;&lt;/Item&gt;&lt;Item&gt;&lt;Description&gt;C#&lt;/Description&gt;&lt;Hint&gt;&lt;/Hint&gt;&lt;ID&gt;ScriptType&lt;/ID&gt;&lt;Text&gt;Script Language&lt;/Text&gt;&lt;Type&gt;String&lt;/Type&gt;&lt;/Item&gt;&lt;/ExecutionData&gt;</ExecutionData><PublishedData datatype="string">&lt;ItemRoot&gt;&lt;Entry&gt;&lt;IsCollection&gt;false&lt;/IsCollection&gt;&lt;Name&gt;results&lt;/Name&gt;&lt;Type&gt;String&lt;/Type&gt;&lt;Variable&gt;results&lt;/Variable&gt;&lt;/Entry&gt;&lt;/ItemRoot&gt;</PublishedData></Object><Object><UniqueID datatype="string">{97579A75-D6FD-4ABA-B460-9437E7E2BC55}</UniqueID><ParentID datatype="string">{C24809F4-1E1B-43F2-B1FB-BD5F4D67264A}</ParentID><Name datatype="string">AD join windows</Name><Description datatype="null"></Description><PositionX datatype="int">471</PositionX><PositionY datatype="int">152</PositionY><ObjectType datatype="string">{ED7F2A41-107A-4B74-BAFE-ADAE63632B79}</ObjectType><SubType datatype="null"></SubType><Enabled datatype="bool">TRUE</Enabled><Flags datatype="null"></Flags><ASC_UseServiceSecurity datatype="null"></ASC_UseServiceSecurity><ASC_ThisAccount datatype="null"></ASC_ThisAccount><ASC_Username datatype="null"></ASC_Username><ASC_Password datatype="null"></ASC_Password><HasExtenders datatype="null"></HasExtenders><CreationTime datatype="date">131744963880000000</CreationTime><CreatedBy datatype="string">S-1-5-500</CreatedBy><LastModified datatype="date">131824276490000000</LastModified><LastModifiedBy datatype="string">S-1-5-21-1549627856-163330409-526660263-86796</LastModifiedBy><Deleted datatype="bool">FALSE</Deleted><Cost datatype="null"></Cost><Savings datatype="null"></Savings><Number datatype="int">0</Number><AlternateDisplayData datatype="null"></AlternateDisplayData><ASW_ObjectTimeout datatype="null"></ASW_ObjectTimeout><ASW_NotifyOnFail datatype="null"></ASW_NotifyOnFail><Flatten datatype="bool">FALSE</Flatten><FlatUseLineBreak datatype="bool">FALSE</FlatUseLineBreak><FlatUseCSV datatype="bool">FALSE</FlatUseCSV><FlatUseCustomSep datatype="bool">TRUE</FlatUseCustomSep><FlatCustomSep datatype="string">,</FlatCustomSep><ObjectTypeName datatype="string">Run .Net Script</ObjectTypeName><ScriptType datatype="string">PowerShell</ScriptType><ScriptBody datatype="string">Powershell {

$ComputerIP = '\`d.T.~Ed/{3835F517-CAB4-41DD-A435-3EDF42C71BE0}.{F8BBB1F9-B1E6-4ACF-B31E-A0592DD8BB83}\`d.T.~Ed/'
$ComputerName = "\`d.T.~Ed/{3835F517-CAB4-41DD-A435-3EDF42C71BE0}.{07714F2E-FF00-4C5C-8084-2AA3BF58AC04}\`d.T.~Ed/"

$user = '\`d.T.~Vb/{B5DDA800-1073-48B6-B940-A633F4ECE9E3}\`d.T.~Vb/'
$password = ConvertTo-SecureString -String '\`d.T.~Vb/{3DC56297-1FE7-4E3C-B424-0332F01D824E}\`d.T.~Vb/' -AsPlainText -Force
$Credential = New-Object -TypeName System.Management.Automation.PSCredential -ArgumentList $user, $password

$administrator = '\`d.T.~Vb/{F92EEDFE-5B96-4E0D-9B47-2D2D5C0A4B99}\`d.T.~Vb/'
$password = ConvertTo-SecureString -String '\`d.T.~Vb/{E7C9BCFB-956C-4A74-8618-8F8A72C59790}\`d.T.~Vb/' -AsPlainText -Force
$LocalCredential = New-Object -TypeName System.Management.Automation.PSCredential -ArgumentList $administrator, $password

